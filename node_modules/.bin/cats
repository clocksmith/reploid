#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/clocksmith/deco/paws/packages/cli-js/bin/node_modules:/home/clocksmith/deco/paws/packages/cli-js/node_modules:/home/clocksmith/deco/paws/packages/node_modules:/home/clocksmith/deco/paws/node_modules:/home/clocksmith/deco/node_modules:/home/clocksmith/node_modules:/home/node_modules:/node_modules:/home/clocksmith/deco/paws/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/clocksmith/deco/paws/packages/cli-js/bin/node_modules:/home/clocksmith/deco/paws/packages/cli-js/node_modules:/home/clocksmith/deco/paws/packages/node_modules:/home/clocksmith/deco/paws/node_modules:/home/clocksmith/deco/node_modules:/home/clocksmith/node_modules:/home/node_modules:/node_modules:/home/clocksmith/deco/paws/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@paws/cli-js/bin/cats.js" "$@"
else
  exec node  "$basedir/../@paws/cli-js/bin/cats.js" "$@"
fi
