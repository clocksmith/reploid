{
  "name": "reploid",
  "version": "0.1.0",
  "description": "Browser-Native AI Agent with Recursive Self-Improvement",
  "main": "boot.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/clocksmith/reploid.git"
  },
  "homepage": "https://github.com/clocksmith/reploid",
  "bin": {
    "reploid": "./bin/reploid-cli.js",
    "reploid-config": "./bin/reploid-config"
  },
  "scripts": {
    "start": "node server.js",
    "cli": "node bin/reploid-cli.js",
    "build:genesis": "node scripts/build-genesis-manifest.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test tests/e2e",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=json"
  },
  "keywords": [
    "reploid",
    "browser",
    "ai",
    "agent",
    "rsi",
    "self-improvement"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clocksmith/doppler": "file:../../../doppler/reploid/doppler",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.16.1",
    "@google/generative-ai": "^0.1.3",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "happy-dom": "^20.0.11",
    "nodemon": "^3.0.1",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
